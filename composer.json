{
  "name": "alijvhr/swoole.io",
  "description": "This Package provides socket.io implementation in php and OpenSwoole",
  "keywords": [
    "swoole",
    "performance",
    "http",
    "websocket",
    "socket server",
    "socket.io",
    "multitasking",
    "server"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "alijvhr",
      "email": "email@example.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "guzzlehttp/promises": "^2.0-stable"
  },
  "require-dev": {
    "openswoole/ide-helper": ">=22.0.1",
    "openswoole/core": ">=22.1.4",
    "phpunit/phpunit": "11.1.x-dev",
    "phpstan/phpstan": "2.0.x-dev",
    "vimeo/psalm": "6.x-dev"
  },
  "autoload": {
    "psr-4": {
      "SwooleIO\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  }
}
