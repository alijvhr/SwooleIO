{
  "name": "alijvhr/swooleio",
  "description": "This Package provides socket.io implementation in php and OpenSwoole",
  "keywords": [
    "swoole",
    "performance",
    "http",
    "websocket",
    "socket server",
    "socket.io",
    "multitasking",
    "server"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "alijvhr",
      "email": "me@alijvhr.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "guzzlehttp/promises": "^2.0-stable",
    "psr/log": "3.x-dev",
    "toolkit/cli-utils": "^2.0-stable",
    "openswoole/core": "*"
  },
  "require-dev": {
    "openswoole/ide-helper": ">=22.0.1",
    "phpunit/phpunit": "^10.5.10"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./openswoole/core"
    }
  ],
  "autoload": {
    "psr-4": {
      "SwooleIO\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  }
}
